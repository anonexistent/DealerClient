<Page x:Class="DealerClient.View.DealerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DealerClient.View"
      xmlns:c="clr-namespace:DealerClient.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="800"
      Title="DealerPage">

    <Page.DataContext>
        <c:MainViewModel/>
    </Page.DataContext>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Command="{Binding GoBackCommand}" Content="back" VerticalAlignment="Top" Padding="10 5" Margin="15" HorizontalAlignment="Left" FontSize="14" FontWeight="Bold"/>
    </Grid>
</Page>
